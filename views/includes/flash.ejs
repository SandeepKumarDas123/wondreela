
<% if(success && success.length)  { %>
<div class="alert alert-info alert-dismissible fade show col-10 offset-1" role="alert">
    <%= success %>
    <button class=" btn-close " data-bs-dismiss="alert" aria-label="close"></button>
</div>

<%  } %>

<% if(error && error.length)  { %>
    <div class="alert alert-danger alert-dismissible fade show col-10 offset-1" role="alert">
        <%= error %>
        <button class=" btn-close " data-bs-dismiss="alert" aria-label="close"></button>
    </div>
    
    <%  } %>

